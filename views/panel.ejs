                                    <i class="fa" onclick="openTab(0)"><% include icons/book-bw.ejs %></i>
                                    <p>book</p>
                                </li>
                                <li>
                                    <i class="fa" onclick="openTab(1)"><% include icons/edit-property.ejs %></i>
